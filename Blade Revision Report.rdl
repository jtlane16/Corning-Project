    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Saws</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@serialnumber">
            <Value>=Parameters!serialnumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Distinct
  Blades.SerialNumber
  ,BladeRevisions.RevisionNumber
  ,ReworkDetails.DateOut
  ,ReworkDetails.DateIn
  ,Blades.InsertTimeStamp AS [Blades InsertTimeStamp]
  ,BladeRevisions.InsertTimeStamp AS [BladeRevisions InsertTimeStamp]
FROM
  BladeRevisions
  INNER JOIN ReworkDetails
    ON BladeRevisions.RevID = ReworkDetails.RevID
  INNER JOIN Blades
    ON BladeRevisions.BladeID = Blades.BladeID
Where serialnumber in (@serialnumber)</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <AutoJoin>False</AutoJoin>
            <SelectedColumns>
              <ColumnExpression ColumnOwner="Blades" ColumnName="SerialNumber" />
              <ColumnExpression ColumnOwner="BladeRevisions" ColumnName="RevisionNumber" />
              <ColumnExpression ColumnOwner="ReworkDetails" ColumnName="DateOut" />
              <ColumnExpression ColumnOwner="ReworkDetails" ColumnName="DateIn" />
              <ColumnExpression ColumnOwner="Blades" ColumnName="InsertTimeStamp" />
              <ColumnExpression ColumnOwner="BladeRevisions" ColumnName="InsertTimeStamp" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="BladeRevisions" />
              <Join Type="INNER JOIN" ColumnOwner="ReworkDetails">
                <On>
                  <From ColumnOwner="BladeRevisions" ColumnName="RevID" />
                  <To ColumnOwner="ReworkDetails" ColumnName="RevID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Blades">
                <On>
                  <From ColumnOwner="BladeRevisions" ColumnName="BladeID" />
                  <To ColumnOwner="Blades" ColumnName="BladeID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="BladeCutCount">
                <On>
                  <From ColumnOwner="BladeRevisions" ColumnName="RevID" />
                  <To ColumnOwner="BladeCutCount" ColumnName="RevID" />
                </On>
                <On>
                  <From ColumnOwner="Blades" ColumnName="BladeID" />
                  <To ColumnOwner="BladeCutCount" ColumnName="BladeID" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
